{
  "version": 1,
  "projects": {
    "kalzate-web": {
      "root": "apps/kalzate-web",
      "sourceRoot": "apps/kalzate-web/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/kalzate-web",
            "index": "apps/kalzate-web/src/index.html",
            "main": "apps/kalzate-web/src/main.tsx",
            "polyfills": "apps/kalzate-web/src/polyfills.ts",
            "tsConfig": "apps/kalzate-web/tsconfig.app.json",
            "assets": [
              "apps/kalzate-web/src/favicon.ico",
              "apps/kalzate-web/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/kalzate-web/src/environments/environment.ts",
                  "with": "apps/kalzate-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "kalzate-web:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "kalzate-web:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/kalzate-web/tsconfig.app.json",
              "apps/kalzate-web/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/kalzate-web/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/kalzate-web/jest.config.js",
            "tsConfig": "apps/kalzate-web/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "kalzate-e2e": {
      "root": "apps/kalzate-e2e",
      "sourceRoot": "apps/kalzate-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/kalzate-e2e/cypress.json",
            "tsConfig": "apps/kalzate-e2e/tsconfig.e2e.json",
            "devServerTarget": "kalzate:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "kalzate:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/kalzate-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/kalzate-e2e/**/*"]
          }
        }
      }
    },
    "landing": {
