      }
    },
    "landing-e2e": {
      "root": "apps/landing-e2e",
      "sourceRoot": "apps/landing-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/landing-e2e/cypress.json",
            "tsConfig": "apps/landing-e2e/tsconfig.e2e.json",
            "devServerTarget": "landing:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "landing:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/landing-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/landing-e2e/**/*"]
          }
        }
      }
    },
    "components": {
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/components/tsconfig.lib.json",
              "libs/components/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/components/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "tsConfig": "libs/components/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "db": {
      "root": "libs/db",
      "sourceRoot": "libs/db/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/db/tsconfig.lib.json",
              "libs/db/tsconfig.spec.json"
            ],
