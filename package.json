{
  "name": "kalzate",
  "version": "0.0.1",
  "description": "Serverless Point Of Sale App For Humans",
  "author": "Zuri Pabon <zurisadai.pabon@gmail.com>",
  "license": "MIT",
  "main": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "build:clean": "npm run test:clean && rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "build:parser": "pegjs -o src/dsl/parser.js src/dsl/grammar.pegjs",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:prod": "npm run test && npm run build && cross-env NODE_ENV=production node server",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint:js": "eslint --ignore-path .eslintignore --ignore-pattern internals/scripts",
    "lint": "npm run lint:js -- . ",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage --config .jestrc",
    "test:watch": "cross-env NODE_ENV=test jest --config .jestrc --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "validate": "npm-run-all --parallel lint test build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "predesktop": "cross-env NODE_ENV=desktop-production webpack --config internals/webpack/webpack.desktop.babel.js --color && cp -r src/desktop/* dist",
    "desktop": "electron-builder",
    "postrelease:desktop": "rimraf dist"
  },
  "dependencies": {
    "babel-polyfill": "6.20.0",
    "csvtojson": "^1.1.9",
    "d3-scale": "^2.1.0",
    "d3-shape": "^1.2.0",
    "d3-time": "^1.0.8",
    "dateformat": "^3.0.3",
    "fontfaceobserver": "2.0.7",
    "immutable": "3.8.1",
    "intl": "1.2.5",
    "invariant": "2.2.2",
    "js-yaml": "^3.11.0",
    "kalzate-db": "../kalzate-db",
    "lodash": "4.17.13",
    "mousetrap": "^1.6.1",
    "pubsub-js": "^1.5.7",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-helmet": "3.2.2",
    "react-intl": "2.1.5",
    "react-mousetrap": "^0.1.1",
    "react-octicon": "^2.0.0",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.6",
    "react-router-scroll": "0.4.1",
    "redux": "3.6.0",
    "reselect": "2.5.4",
    "sanitize.css": "4.1.0",
    "styled-components": "2.1.2",
    "warning": "3.0.0",
    "whatwg-fetch": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-dynamic-import-node": "1.0.0",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "circular-dependency-plugin": "2.0.0",
    "compression": "1.6.2",
    "coveralls": "2.11.15",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.2",
    "enzyme": "2.6.0",
    "eslint": "3.11.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.7.1",
    "eslint-plugin-redux-saga": "0.1.5",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.x",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "ip": "1.1.4",
    "jest-cli": "18.0.0",
    "lint-staged": "3.2.1",
    "minimist": "1.2.0",
    "ngrok": "2.2.4",
    "node-plop": "0.5.4",
    "null-loader": "0.1.1",
    "offline-plugin": "4.5.2",
    "pegjs": "^0.10.0",
    "plop": "1.7.3",
    "prettier-eslint": "^6.4.2",
    "react-addons-test-utils": "15.4.1",
    "redux-saga": "^0.16.0",
    "rimraf": "2.5.4",
    "shelljs": "0.7.5",
    "sinon": "2.0.0-pre",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0"
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css",
      "react-octicon",
      "kalzate-db"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "build": {
    "appId": "com.kalzate",
    "productName": "Kalzate",
    "copyright": "Copyright Â© 2018 ${author}",
    "win": {
      "target": [
        {
          "target": "portable"
        }
      ],
      "icon": "dist/kalzate-icon.ico",
      "legalTrademarks": "Github"
    },
    "linux": {
      "target": [
        {
          "target": "deb"
        },
        {
          "target": "zip"
        }
      ],
      "icon": "dist/kalzate-icon.ico",
      "category": "Development"
    },
    "mac": {
      "target": [
        {
          "target": "dmg"
        },
        {
          "target": "zip"
        }
      ],
      "icon": "dist/kalzate-icon.ico"
    },
    "directories": {
      "output": "release",
      "app": "dist"
    }
  }
}